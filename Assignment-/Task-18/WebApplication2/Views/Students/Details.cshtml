@model WebApplication2.Models.Student
<h2>Student Details</h2>

<p><b>ID:</b> @Model.Id</p>
<p><b>Name:</b> @Model.Name</p>
<p><b>Age:</b> @Model.Age</p>
<p><b>Course:</b> @Model.Course</p>
<p><b>Marks:</b> @Model.Marks</p>
<p><b>Date of Birth:</b> @Model.DateOfBirth.ToString("yyyy-MM-dd")</p>

